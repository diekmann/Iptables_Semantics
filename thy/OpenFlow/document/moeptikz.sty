	\pgfpathlineto{\pgfpoint{-\pgf@xa}{-.5\pgf@ya}}
	\pgfpathlineto{\pgfpoint{-.25\pgf@xa}{-.5\pgf@ya}}
	\pgfpathlineto{\pgfpoint{-.25\pgf@xa}{-.79\pgf@ya}}
	\pgfpathlineto{\pgfpoint{-.4\pgf@xa}{-\pgf@ya}}
